{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [ "es2017", "dom" ],
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist_ts",
    "strict": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules"
  ]
}
